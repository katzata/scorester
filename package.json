{
  "name": "scorester",
  "version": "2.0.0",
  "description": "A score keeping app",
  "keywords": [
    "score",
    "games",
    "utils"
  ],
  "proxy": "https://192.168.0.185/:8080",
  "license": "MIT",
  "author": {
    "name": "Kamen Kashchiev",
    "email": "kamen@kashchiev.com",
    "url": "https://www.kashchiev.com"
  },
  "repository": "github:katzata/scorester",
  "dependencies": {
    "crypto-js": "^4.1.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-scripts": "5.0.1",
    "sass": "^1.56.0",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "X": "node handleAppVersion.js",
    "start": "env-cmd -e development react-scripts start",
    "build": "env-cmd -e build react-scripts build",
    "serve": "serve -l 5000 -s build",
    "build-ver": "node handleAppVersion.js && env-cmd -e build react-scripts build",
    "test": "npx cypress open",
    "eject": "react-scripts eject",
    "push-q": "git add . && git commit -m \"Minor changes commit.\" && git push"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.20.12",
    "@babel/plugin-syntax-jsx": "^7.18.6",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-react": "^7.18.6",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^14.4.3",
    "babel-jest": "^29.3.1",
    "cypress": "^12.3.0",
    "env-cmd": "^10.1.0",
    "jest": "^27.5.1",
    "react-test-renderer": "^18.2.0"
  }
}
